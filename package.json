{
  "name": "prisma-cli-extension",
  "main": "dist/index.js",
  "files": [
    "dist/**",
    "types/**"
  ],
  "types": "types/index.d.ts",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloVSouza/prisma-cli-extension.git"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/entrypoint.ts",
    "fix-path": "npx tsc-alias tsconfig-build.json",
    "build": "tsc --project tsconfig.json && npm run fix-path",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "clean": "rm -rf dist coverage",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build && npm run test:ci",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "check": "npm run tsc && npm run lint && npm run test",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@prisma/client": "^6.15.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@prisma/client": "^6.15.0"
  },
  "dependencies": {
    "@electron/asar": "^3.4.1"
  },
  "packageManager": "npm@10.0.0"
}
